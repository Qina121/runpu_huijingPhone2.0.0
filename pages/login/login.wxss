.style-form {
    height: 390rpx;
    width: 100%;
    position: relative;
    display: block;
}
.weui-input {
    position: relative;
    width:90%;
    border:1px solid #C7C7CC;
    margin-left: 5%;
    border-radius:33rpx;
    margin-bottom: 40rpx;
    box-sizing: border-box;
    height: 65rpx;
    line-height: 65rpx;
    font-size: 26rpx;
    padding-left: 5%;

}
.submit-login{
    background-color: #A60001 !important;
    color: white;
    margin: 5% ;
    margin-top: 80rpx;
    border-radius: 40rpx;
    margin-bottom: 50rpx;
    box-shadow:0rpx 10rpx 27rpx 3rpx rgba(166,0,1,0.3);
    width:90% !important;
    height: 65rpx;
    font-size: 30rpx;
    box-sizing: border-box;
}
.page-header {
    width: 100%;
    height: 400rpx;
    position: relative;
}
.weui-input:hover {
    border:1px solid #A60001;
}
.backGround-box {
    position: absolute;
    width:100%;
    height: 400rpx;
    top: -50rpx;
}
.logoImage {
    width: 100%;
    height: 500rpx;
    position: relative;
}
.center-box {
    position: absolute;
    top:46%;
    left:50%;
    transform: translateX(-50%);
    width: 240rpx;
    height: 240rpx;
    border-radius:30rpx;
}
.newUser {
    height:40rpx;
    font-size:24rpx;
    color:rgba(166,0,1,1);
    display: block;
    width: 20%;
    line-height: 40rpx;
    position: absolute;
    left:50%;
    transform: translateX(-50%);
}
.boso {
    position: relative;
    
}
.iconColor {
    font-size: 38rpx;
    position: absolute;
    top:2rpx;
    right:8%;
    color:#A60001;
    width: 60rpx;
    height:60rpx;
    z-index: 21;
}
.moren {
    width: 100%;
    margin-bottom: 40rpx;
    height: 65rpx;
    position: relative;
}