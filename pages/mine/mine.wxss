.pages {
    width: 100%;
    height: 100%;
    background-color: #fafafa;
}
.page-body {
    position: relative;
    margin:0rpx;
    padding:0rpx;
}
.page-header {
    width: 100%;
    height: 500rpx;
    position: relative;
    
}
.logoImage {
    width: 100%;
    height: 350rpx;
    position: relative;
}
.backGround-box {
    position: absolute;
    width: 100%;
    height: 330rpx;
    top: -50rpx;
}
.center-box {
    position: absolute;
    top:4%;
    left:50%;
    transform: translateX(-50%);
    width: 90%;
    height: 300rpx;
    border-radius:30rpx;
    z-index: 20;
}
.showThree {
    font-size:24rpx;
    width: 100%;
    text-align: center;

}
.page-section-spacing {
    width: 100%;
    height: 180rpx;
    padding:5%;
    position: absolute;
    top:30%;
    left:50%;
    transform: translateX(-50%);
    box-sizing: border-box;
}
.allbox {
    width: 100%;
    height: 180rpx;
    background-color: #FFF;
    padding: 3% ;
    box-shadow:0rpx 0rpx 18rpx 2rpx rgba(0, 0, 0, 0.3);
    box-sizing: border-box;
    border-radius:20rpx;
    position: relative;
}
.smallbox {
    text-align: center;
    width: 33%;
    height: 100%;
    float: left;
    position: relative;
}
.examination {
    display: block;
    width: 100%;
    font-size:60rpx;
    text-align: center;
    font-family:Impact;
    font-weight: 700;
}
.showone {
    border-left:1rpx solid rgba(238,238,238,1);

}
.boxOne {
    color:#EC9F05;
}
.boxtwo {
    color: #A60001;
}
.boxThree {
    color: #47A025
}
.paddinga {
    border-bottom: 1rpx solid #EFEFF4 ;
    margin-bottom: 1rpx ;
}
.paddings {
    margin-bottom: 20rpx;
}
.xiangxi {
    width: 100%;
    text-indent: 3rem;
    font-size: 28rpx;
    height: 80rpx;
    position: relative;
}
.showinline {
    text-align: right;
    display: block;
    border-left:1rpx solid rgba(238,238,238,1);
    position: absolute;
    right:0rpx;
    top:20%;
    height: 60%;
    color:#fff
}
.mines {
    display: block;
    width: 100%;
    font-size:28rpx;
    font-weight:400;
    color:rgba(51,51,51,1);
    line-height:80rpx;
    background-color: #fff;
    height: 80rpx;
}
.styles {
    position: absolute;
    width: 90%;
    margin-left: 5%;
    z-index: 40;
    color:#fff;
    height: 300rpx;
    box-sizing: border-box;
    padding-top: 50rpx;
    padding-left: 20rpx;
}
.mini-boxs {
    position: absolute;
    right:0rpx;
    top:24%;
    width: 160rpx;
    height: 48rpx;
}
.userimgbox {
    width: 200rpx;
    height: 200rpx;
}
.disablesd {
    display: block;
    width: 30%;
    height: 40rpx;
    position: absolute;
    margin-left:32%;
    top:30%;
    font-size: 36rpx;
    font-weight: 600;
}
.twods {
    font-size: 24rpx;
    top:56%;
    border: 1rpx solid #fff;
    border-radius:21rpx;
    width: 20%;
    text-align: center;
}
.widths {
    margin-top:20rpx
}
.iconColor {
    position: absolute;
    left:-40rpx;
    color:#A60001;
    line-height: 82rpx;
    width: 40rpx;
    height: 40rpx;
    font-weight: 700;
}
.iconright {
    position: absolute;
    line-height: 80rpx;
    right:20rpx;
    float: right;
    top:0rpx;
    color:#E5E5E5;
    z-index: 60;
    font-size: 40rpx;
}
.lastsheh {
    display: block;
    width: 100%;
    height: 20rpx;
    background-color: #FAFAFA;
}

