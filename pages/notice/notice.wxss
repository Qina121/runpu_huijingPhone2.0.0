.page {
    width: 100%;
    height: 100%;
    position: relative;
    margin: 0;
    padding: 0;
    background-color: #FAFAFA;
}
.bgImage {
    width:750rpx;
    height:280rpx;
}
.pagecenter {
    position: relative;
    width:750rpx;
    height:800rpx;
}
.pagecenterTop {
    width:670rpx;
    height:800rpx;
    background:rgba(255,255,255,1);
    box-shadow:0rpx 0rpx 18rpx 2rpx rgba(0, 0, 0, 0.3);
    border-radius:20rpx; 
    position: relative;
    left:50%;
    transform: translateX(-50%);
    margin-top: 30rpx;
    padding: 0rpx 36rpx;
    box-sizing: border-box;
}
.newNotice {
    display: block;
    width:126rpx;
    height:100rpx;
    font-size:40rpx;
    font-weight:bold;
    color:rgba(166,0,1,1);
    width: 100%;
    text-align: center;
    line-height: 100rpx;
    margin-bottom: 16rpx;
    position: relative;
}
.inline {
    position: absolute;
    top: 88rpx;
    height: 10rpx;
    left:50%;
    transform: translateX(-50%);
    margin-bottom: 30rpx
}
.pagecenters {
    width:600rpx;
    height:260rpx;
    overflow: hidden;
}
.sectionImage {
    width:600rpx;
    height:260rpx;
    background:rgba(102,102,102,1);
    position: absolute;
    left:50%;
    transform: translateX(-50%);
}
.pagecenterBottom {
    width:600rpx;
    height:223rpx;
    background:rgba(255,255,255,1);
    margin: 40rpx 0rpx;
    box-sizing: border-box;
    font-size: 24rpx;
    letter-spacing: 4rpx;
    text-indent: 48rpx;
    text-align: justify;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
    overflow: hidden;
    color:#000000
}
.pagecenterEnd {
    width:600rpx;
    height:65rpx;
    background:rgba(166,0,1,1);
    box-shadow:0rpx 10rpx 27rpx 3rpx rgba(166,0,1,0.3);
    border-radius:33rpx;
}
.noticess {
    color: white;
    text-align: center;
    display: block;
    height:65rpx;
    line-height: 65rpx;
    font-weight: 700;
    font-size: 24rpx;
}
.pagefooter {
    width: 100%;
}
.historyNotice {
    display: block;
    width:750rpx;
    height:30rpx;
    font-size:30rpx;
    font-weight:bold;
    color:rgba(166,0,1,1);
    line-height:40rpx;
    border-left: 4rpx solid #600001;
    box-sizing: border-box;
    margin: 40rpx 40rpx;
    line-height: 30rpx;
    padding-left: 20rpx;
}
.allText {
    margin: 40rpx;
    width: 670rpx;
    box-sizing: border-box;
    height: 240rpx;
    background-color: #fff;
    border:1rpx solid #EEEEEE;
    border-radius:20rpx;

}
.styleBlock {
    display: block;
    width: 670rpx;
    height: 240rpx;
    position: relative;
}
.titleStyle {
    font-weight:bold;
    color:rgba(51,51,51,1);
    line-height:80rpx;
    font-size:26rpx;
    width:670rpx;
    height: 80rpx;
    padding: 0rpx 35rpx;
    box-sizing: border-box;
    border-bottom: 4rpx solid #EEEEEE;
    margin-bottom: 16rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis; 
}
.textEee {
    width:598rpx;
    height:70rpx;
    font-size:24rpx;
    font-weight:400;
    color:rgba(136,136,136,1);
    padding: 0rpx 35rpx;
    line-height:34rpx;
    overflow: hidden;
    white-space:nowrap;
    text-overflow:ellipsis
}
.text-box {
    position: relative;
    width: 50%;
    padding: 0rpx 35rpx;
    height: 24rpx;
    margin-bottom: 10rpx;

}
.texts {
    font-size:24rpx;
    height: 40rpx;
    color:rgba(221,221,221,1);
    line-height:40rpx;
    padding: 0rpx 35rpx;
    font-family:Impact;
    float: left;
    margin-top:10rpx;
    margin-left: 40rpx;


}
.noticeslist {
    float: right;
}
.same {
    color:#018E42;
    display: block;
    width: 100%;
    text-align: center;
    position: absolute;
    top:10%;
    font-size: 24rpx;
}
.pageHeader {
    position: relative;
}
.detailchina {
    top:20%;
    font-size: 50rpx;
    font-weight: 700;
}
.detailchinas {
    top:50%;
    width: 10%;
    border: 2rpx solid #018E42;
    left: 50%;
    transform: translateX(-50%);
}
.iconColor {
    position: absolute;
    color:#A60001;
    font-size: 24rpx;
    float: left;
    left: 35rpx;
    margin-top:10rpx;
}
.iconColorright {
    float: right;
    color:#A60001;
    margin-right: 30rpx;
    border: 1rpx solid #A60001;
    border-radius: 100%;
    width: 36rpx;
    height: 36rpx;
    box-sizing: border-box;
    text-align: center;
    line-height: 36rpx;

}