.page {
    width: 100%;
    height: 100%;
    position: relative;
    margin: 0;
    padding: 0;
    background-color: #FAFAFA;
}
.pageHeader {
    position: relative;
    color:#018E42;
}
.bgImage {
    width:750rpx;
    height:280rpx;
}
.pageCenter{
    width: 90%;
    height: 100%;
    padding: 40rpx;
    
}
.titleOne {
    width: 100%;
    border-bottom: 1rpx solid #E3E4E4;
    text-align: center;
    font-weight:bold;
    line-height:20rpx;
    font-size:32rpx;
    box-sizing: border-box;
    height: 60rpx;
}
.titleNote {
    height:80rpx;
    font-size:20rpx;
    font-weight:400;
    color:rgba(136,136,136,1);
    line-height:80rpx;
    border-bottom: 1rpx solid #E3E4E4;
    letter-spacing:5rpx;
}
.containner {
    width: 100%;
    font-size:24rpx;
    font-weight:400;
    color:rgba(102,102,102,1);
    text-indent: 2em;
}
.noticeImage {
    width: 100%;
    margin-top: 30rpx;
}
.pageFooter {
    width: 90%;
    padding: 40rpx;
}
.noticeUP {
    width: 100%;
    height: 60rpx;
    background:rgba(255,255,255,1);
    border:1rpx solid rgba(238,238,238,1);
    border-radius:10rpx;
    font-size: 24rpx;
    padding-left:20rpx;
    line-height: 60rpx;
    box-sizing: border-box;
    margin-bottom: 20rpx;

}
.noticeUP:hover{
    color:#A60001
}
.same {
    color:#018E42;
    display: block;
    width: 100%;
    text-align: center;
    position: absolute;
    top:10%;
    font-size: 24rpx;

}
.pageHeader {
    position: relative;
}
.detailchina {
    top:20%;
    font-size: 50rpx;
    font-weight: 700;
}
.detailchinas {
    top:50%;
    width: 10%;
    border: 2rpx solid #018E42;
    left: 50%;
    transform: translateX(-50%);
}
.repairdetail {
    width: 100%;
    font-size: 24rpx;
    font-weight: 700;
    color:#666666;
    height: 60rpx;
    line-height: 60rpx;

}
.username {
    width: 60%;
    height: 200rpx;
    font-weight: 400;
}
.heightd {
    height: 200rpx;
    text-align: justify;
}