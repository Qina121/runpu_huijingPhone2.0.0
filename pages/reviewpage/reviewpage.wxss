.page {
    position: relative;
    margin:0rpx;
    padding-top:0rpx;
    width: 100%;
    height: 100%;
}
.page_title{
    width: 100%;
    height: 200rpx;
    position: absolute;
    z-index: 99;
    /* text-align: center; */
    display: flex;
    align-items: center;
}
.page_name{
    color: #fff;
    font-weight: bold;
    /* line-height: 187rpx; */
    margin-bottom: -60rpx;
    /* text-align: center; */
    margin-top: -61rpx;
    margin-left: 16px;

}
.page_title_backgrond{
    width: 200rpx;
    height: 65rpx;
    margin-left: 40rpx;
}
.page-header {
    width: 100%;
    height: 600rpx;
    position: relative;
}
.style-form {
    height: 390rpx;
    width: 750rpx;
    position: relative;
    display: block;
}
.weui-input {
    position: relative;
    width:600rpx;
    border:1px solid #C7C7CC;
    margin-left: 74rpx;
    border-radius:33rpx;
    margin-bottom: 40rpx;
    box-sizing: border-box;
    height: 65rpx;
    line-height: 65rpx;
    font-size: 26rpx;
    text-indent: 3%;
    padding-left: 20rpx;

}
.weui-input:hover {
    border:1px solid #A60001;
}
.submit-login{
    margin-top: 80rpx;
    background-color: #A60001;
    color: white;
    padding: 0;
    display: block;
    margin-bottom: 50rpx;
    box-shadow:0px 10px 27px 3px rgba(166,0,1,0.3);
    border:0px solid #C7C7CC;
    width:600rpx;
}
.endInput {
    margin-bottom: 40rpx;
}
  .button-sp-area{
    margin: 0 auto;
    width: 100%;
  }
  .text-box {
      height: 25rpx;
      display: inline-block;
      width: 140rpx;
      box-sizing: border-box;
      
  }
  .first-box {
    color:#C7C7CC;
    margin-left:234rpx;
    font-size: 24rpx;
    text-align: left;
  }
  .second-box {
    color:#A60001;
    font-size: 24rpx;
    text-align: right;
  }
  .iconColor {
    display: block;
    font-size: 120rpx;
    position: absolute;
    top:10%;
    color:#fff;
    width: 100%;
    height: 50%;
    text-align: center;
}
.first-line {
    color:#C7C7CC;
    text-align: left;
}
.container{
    width: 750rpx;
    height: 174rpx;
    margin: 0;
    padding: 0;
    position: relative;
  }
  .page-footer{
    position: fixed;
    width:100%;
    height: 174rpx;
    bottom:0rpx
  }
  .center-box {
      position: absolute;
      top:40%;
      left:50%;
      transform: translateX(-50%);
      width: 200rpx;
      height: 200rpx;
      border-radius:30rpx;
  }
  .backGround-box {
      position: absolute;
      width: 750rpx;
      height: 400rpx;
      top: -50rpx;

  }
.logoImage {
    width: 100%;
    height: 500rpx;
    position: relative;

}
.pageContainer {
    position: relative;
}
.big-box {
    position: relative;
    width: 320rpx;
    height: 334rpx;
    background-color: #A60001;
    border-radius:20rpx;
    margin-left: 40rpx;
    color: #FFF;
    float: left;
    margin-bottom: 30rpx;
}
.boxColor {
    position: absolute;
    bottom:50rpx;
    left:50%;
    transform: translateX(-50%);
    font-size: 36rpx;
}
.pageCenter {
    position: absolute;
    width: 100%;
    height: 400rpx;
    top: 30%
}
.bgImage {
    width: 100%;
    height: 100%;
    overflow: hidden;
}
.lunboone {
    position: relative;
}
.imgItem{
    width:800rpx;
    height:400rpx;
    border-radius:20rpx;
    overflow:hidden;
}
.slide-image{
    /* height: 400rpx; */
    width: 90%;
    border-radius:20rpx;
    position: absolute;
    left:50%;
    transform: translateX(-50%);
}
.boxHeight {
    height: 380rpx;
    
}
  

  